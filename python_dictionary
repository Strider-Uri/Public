roster = {
		"Eric Cartmon" : 0,
		"Joshua Morgan" : 0,
		"Job" : 25,
		"Colonel Sanders" : 1
		}

for item in roster:
    raw_input("Enter round points for " + item + ".")
    print item + ": " + str(roster[item])

""" simple dict user input """

